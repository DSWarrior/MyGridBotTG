<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationIcon>LogBoVi.ico</ApplicationIcon>
    <Authors>$(BoVi)</Authors>
    <Copyright>BoVi</Copyright>
    <AssemblyName>$(MSBuildProjectName)</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="LogBoVi.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Bybit.Net" Version="3.13.1" />
    <PackageReference Include="ClosedXML.Report" Version="0.2.3" />
    <PackageReference Include="JK.Mexc.Net" Version="1.7.2" />
  </ItemGroup>

</Project>
